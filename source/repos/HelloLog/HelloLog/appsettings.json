{
	"Serilog": {
        "Using":  [ "Serilog.Sinks.Console", "Serilog.Sinks.RollingFile" ],
        "WriteTo": [
            { "Name": "Console" },
            { "Name": "RollingFile", "Args": {"pathFormat": "Log/log-{Date}.txt", "fileSizeLimitBytes": "1000000", "retainedFileCountLimit": "10", "rollOnFileSizeLimit": "true" } }
        ]
	}
}